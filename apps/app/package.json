{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm nx serve:app",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract-intl": "formatjs extract 'src/**/!(*.d).(js|jsx|ts|tsx)' '../../packages/core/src/**/!(*.d).(js|jsx|ts|tsx)' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file ../../packages/core/src/translations/en.json --ignore src/tests/mocks/**",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "vercel-build": "next build && pnpm run db:migrate",
    "db:migrate": "cd ../../packages/schema && pnpm run db:migrate"
  },
  "dependencies": {
    "@ab/api-client": "*",
    "@ab/core": "*",
    "@apollo/client": "^3.8.4",
    "@apollo/experimental-nextjs-app-support": "^0.4.1",
    "@apollo/server": "^4.7.5",
    "@as-integrations/next": "^2.0.2",
    "@auth/typeorm-adapter": "^1.0.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@formatjs/intl-localematcher": "^0.4.2",
    "@heroicons/react": "^2.0.17",
    "@mantine/core": "^6.0.11",
    "@mantine/form": "^6.0.17",
    "@mantine/hooks": "^6.0.11",
    "@mantine/modals": "^6.0.17",
    "@mantine/next": "^6.0.11",
    "@mantine/notifications": "^6.0.19",
    "@tabler/icons-react": "^2.30.0",
    "@types/react-dom": "18.2.1",
    "eslint": "8.39.0",
    "graphql": "^16.6.0",
    "negotiator": "^0.6.3",
    "next": "13.4.18",
    "next-auth": "^4.22.3",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.4.7",
    "sharp": "^0.32.6",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.0",
    "@next/eslint-plugin-next": "^13.4.19",
    "@nx/jest": "^16.6.0",
    "@storybook/cli": "^7.2.2",
    "@storybook/react": "^7.2.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.0",
    "@types/negotiator": "^0.6.1",
    "@types/node": "20.6.3",
    "eslint-config-next": "13.4.19",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-matcher-utils": "^29.6.2",
    "jest-watch-typeahead": "^2.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "^7.2.2",
    "storybook-dark-mode": "^3.0.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
